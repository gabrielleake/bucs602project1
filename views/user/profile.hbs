
<div class="row">
    <div class="col-md-9 col-md-offset-3">
        <h1>User Profile</h1>
        <hr>
        <h2>Order History:</h2>
        <div class="panel panel-default">
            {{# each orders }}
                <div class="panel-body">
                    <ul class="list-group">
                        {{# each this.items }}
                            <li class="list-group-item">
                                <span class="badge">${{ this.price }}</span>
                                {{ this.item.title }} | {{ this.qty }} (qty)
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="panel-footer">${{ this.cart.total }}</div>
            {{/each}}

        </div>
    </div>
</div>